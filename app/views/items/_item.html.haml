.item
  %ul
    %li.item-collect.pull-right
      =render partial: 'shared/collect_button', locals: {item: item}
    %li.item-title
      %h4= link_to item.title, item_path(item), remote:true

    / # %li#item_authors= item.authors
    %li#item_category
      %span Matches with
      - item.tags.each do |tag|
        %span.tag= tag.name




